public StartCountDown()
{
	CountStatus = 1;
	for(new i = 0; i < MAX_PLAYERS; i++)
	{
	    if(IsAtEvent[i] == 1)
	    {
			GameTextForAll("~r~Get Ready!", 1000, 5);
			SetTimer("CountDownStage1", 1000, false);
	    }
	}
	return 1;
}

